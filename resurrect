[Service] Type=oneshot User=tkg_h30_2018 Environment=PM2_HOME=/home/tkg_h30_2018/.pm2 ExecStart=/usr/bin/pm2 resurrect RemainAfterExit=yes
[Install] WantedBy=multi-user.target UNIT
sudo systemctl daemon-reload sudo systemctl enable --now pm2-resurrect sudo systemctl status pm2-resurrect --no-pager pm2 ls
