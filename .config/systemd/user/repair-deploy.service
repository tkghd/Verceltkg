[Unit]
Description=TKGHD Repair & Internal Start (User Service)
After=network.target
[Service]
Type=simple
WorkingDirectory=/home/tkg_h30_2018
ExecStart=/home/tkg_h30_2018/scripts/repair-deploy.sh
Restart=always
RestartSec=10
EnvironmentFile=/home/tkg_h30_2018/.env
StandardOutput=append:/home/tkg_h30_2018/logs/systemd.out.log
StandardError=append:/home/tkg_h30_2018/logs/systemd.err.log
[Install]
WantedBy=default.target
